    "googleapis": "^128.0.0",
    "heic2any": "^0.0.4",
    "jszip": "^3.10.1",
    "lucide-react": "^0.294.0",
    "motion": "^12.23.22",
    "multer": "^2.0.2",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
